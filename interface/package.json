{
  "name": "deepsee",
  "version": "0.1.0",
  "author": "Adam Coscia",
  "description": "DeepSee",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "node server.js",
    "start-electron": "vue-cli-service build && electron .",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-electron": "vue-cli-service build && electron-builder",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "d3": "^7.0.0",
    "express": "^4.17.1",
    "jquery-csv": "^1.0.21",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "three": "^0.130.1",
    "vsup": "^1.1.0",
    "vue": "^2.6.11",
    "vuetify": "^2.4.0"
  },
  "devDependencies": {
    "@mdi/js": "^5.9.55",
    "@types/d3": "^7.0.0",
    "@types/lodash": "^4.14.172",
    "@types/three": "^0.130.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "electron": "^13.2.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": "16.x"
  },
  "build": {
    "appId": "com.electron.deepsee",
    "productName": "DeepSee",
    "copyright": "Copyright Â© 2022-23 California Institute of Technology (\"Caltech\")",
    "mac": {
      "icon": "./public/icons/mac.icns",
      "category": "public.app-category.education"
    },
    "win": {
      "icon": "./public/icons/win.ico"
    },
    "directories": {
      "buildResources": "./public",
      "output": "./app"
    },
    "asar": true,
    "extraResources": [
      {
        "from": "./dist/",
        "to": ".",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
